<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <com.cylan.jiafeigou.widget.CustomToolbar
        android:id="@+id/custom_toolbar"
        layout="@layout/layout_default_custom_tool_bar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:ct_enable_theme="true"
        app:ct_right_title="@string/FINISHED"
        app:ct_title="@string/NAME" />

    <com.cylan.jiafeigou.widget.ClearableEditText
        android:id="@+id/edit_face_name"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />

</LinearLayout>
