    <style name="warn_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_down_in</item>
        <item name="android:windowExitAnimation">@anim/slide_down_out</item>
    </style>

    <style name="activity_anim" parent="android:Animation">
        <item name="android:windowEnterAnimation">@anim/slide_right_in</item>
        <item name="android:windowExitAnimation">@anim/slide_right_out</item>
