    <style name="setting_text">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:textSize">@dimen/area_code_size</item>
        <item name="android:textColor">#000000</item>
        <item name="android:paddingLeft">@dimen/login_edit_magin</item>
        <item name="android:gravity">left|center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:drawablePadding">@dimen/login_edit_magin</item>
        <item name="android:paddingRight">15dip</item>
        <item name="android:paddingTop">10dp</item>
        <item name="android:paddingBottom">10dp</item>
    </style>

    <style name="setting_text_value">
        <item name="android:lines">1</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#adadad</item>
        <item name="android:paddingRight">@dimen/login_edit_magin</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:gravity">left|center</item>
        <item name="android:drawablePadding">4dip</item>
    </style>

