<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DotThemes">
        <attr name="position" format="integer" />
        <attr name="ignore" format="boolean" />
        <attr name="Dradius" format="dimension" />
        <attr name="margin" format="dimension" />
        <attr name="showDot" format="boolean" />
        <attr name="tipDrawable" format="reference" />
        <attr name="borderColor" format="color" />
        <attr name="borderWidth" format="dimension" />
        <attr name="enableBorder" format="boolean" />
    </declare-styleable>
    <declare-styleable name="WaveTheme">
        <attr name="frontColor" format="color" />
        <attr name="behindColor" format="color" />
    </declare-styleable>
    <declare-styleable name="SuperWaveTheme">
        <attr name="waveColor" format="color|reference" />
        <attr name="waveCount" format="integer" />
    </declare-styleable>
    <declare-styleable name="HomeMineItemViewStyle">
        <attr name="leftDrawable" format="reference" />
        <attr name="rightDrawable" format="reference" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color|integer" />
        <attr name="text" format="string" />
    </declare-styleable>
    <declare-styleable name="FateLineViewStyle">
        <attr name="dashLineWidth" format="integer|dimension" />
        <attr name="dashLineColor" format="color|reference" />
        <attr name="outerCircleColor" format="color|reference" />
        <attr name="innerCircleColor" format="color|reference" />
        <attr name="outerCircleRadius" format="integer|dimension" />
        <attr name="innerCircleRadius" format="integer|dimension" />
        <attr name="innerCircleStrokeWidth" format="float|dimension" />
        <attr name="outerCircleY" format="float|dimension" />
    </declare-styleable>
    <declare-styleable name="WheelViewStyle">
        <attr name="shortLineHeight" format="float|dimension" />
        <attr name="longLineHeight" format="float|dimension" />
    </declare-styleable>
    <declare-styleable name="ZXingScannerViewStyle">
        <attr name="hint" format="reference|string" />
    </declare-styleable>
    <declare-styleable name="RoundedImageViewStyle">
        <attr name="riv_corner_radius" format="dimension" />
        <attr name="riv_corner_radius_top_left" format="dimension" />
        <attr name="riv_corner_radius_top_right" format="dimension" />
        <attr name="riv_corner_radius_bottom_left" format="dimension" />
        <attr name="riv_corner_radius_bottom_right" format="dimension" />
        <attr name="riv_border_width" format="dimension" />
        <attr name="riv_border_color" format="color" />
        <attr name="riv_mutate_background" format="boolean" />
        <attr name="riv_oval" format="boolean" />
        <attr name="android:scaleType" />
        <attr name="riv_tile_mode">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_x">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
        <attr name="riv_tile_mode_y">
            <enum name="clamp" value="0" />
            <enum name="repeat" value="1" />
            <enum name="mirror" value="2" />
        </attr>
    </declare-styleable>
    <!--add by hunt -->
    <declare-styleable name="ScanEffectViewStyle">
        <attr name="effectStartColor" format="reference|color" />
        <attr name="effectEndColor" format="reference|color" />
        <attr name="laserWidth" format="integer|dimension" />
        <attr name="duration" format="integer|reference" />
    </declare-styleable>

    <declare-styleable name="PagerSlidingTabStrip">
        <attr name="pstsIndicatorColor" format="color" />
        <attr name="pstsUnderlineColor" format="color" />
        <attr name="pstsDividerColor" format="color" />
        <attr name="pstsIndicatorHeight" format="dimension" />
        <attr name="pstsUnderlineHeight" format="dimension" />
        <attr name="pstsDividerPadding" format="dimension" />
        <attr name="pstsTabPaddingLeftRight" format="dimension" />
        <attr name="pstsScrollOffset" format="dimension" />
        <attr name="pstsTabBackground" format="reference" />
        <attr name="pstsShouldExpand" format="boolean" />
        <attr name="pstsTextAllCaps" format="boolean" />
    </declare-styleable>

    <declare-styleable name="SimpleProgressStyle">
        <attr name="progress_stroke_width" format="dimension" />
        <attr name="progress_stroke_color" format="reference|color" />
        <attr name="progress_point_radius" format="dimension" />
    </declare-styleable>
</resources>